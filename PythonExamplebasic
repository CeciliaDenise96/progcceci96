#-*- coding: latin-1 -*-
import tkinter as tk
from tkinter import*
import sqlite3
win = Tk()
win.geometry("700x350")
class Mi_Proceso():
    def __init__(self):
        self.edad = edad
    def Proceso(self):
        Tk.Label(win, text="¿Cual es tu edad?").pack()
        Tk.Button(win, text="Tu edad es:", command='Proceso').pack()
        edad = Tk.Entry(str(root))
        edad.pack()
    def ventana(self):
        self = self
        self.edad = edad
        cantidad = int(input("¿Cual es tu edad?"))
win.mainloop()
